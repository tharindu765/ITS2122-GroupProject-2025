   "outputs": [],
   "source": [
    "print(\"missing CustomerID before:\", df['CustomerID'].isnull().sum())\n",
    "\n",
    "# Option 1: drop rows missing CustomerID (needed for RFM later)\n",
    "df_with_cust = df.dropna(subset=['CustomerID']).copy()\n",
